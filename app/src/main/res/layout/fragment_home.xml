        </RelativeLayout>
        <RelativeLayout
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:background="@drawable/fragment_form"
                android:padding="6.0dip"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
