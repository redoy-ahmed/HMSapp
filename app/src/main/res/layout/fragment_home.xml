                    android:layout_toRightOf="@id/list_image17">
                <TableRow
                        android:id="@+id/tableRow38"
                        android:padding="4.0dip"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">
                    <TextView
                            android:textColor="#ff000000"
